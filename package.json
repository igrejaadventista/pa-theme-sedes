{
  "name": "pa-thema-sedes",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=development run-s mix",
    "build:production": "mix --production",
    "start": "cross-env NODE_ENV=development run-s \"mix -- --watch\"",
    "hot": "cross-env NODE_ENV=development run-s build mix:hot",
    "mix": "webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "mix:hot": "webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "clean": "run-p clean:*",
    "clean:dist": "rimraf dist",
    "lint": "run-s -c lint:*",
    "lint:scripts": "eslint assets/scripts",
    "lint:styles": "stylelint \"assets/**/*.{vue,css,sass,scss,less}\"",
    "test": "run-s -c lint"
  },
  "devDependencies": {
    "@tinypixelco/laravel-mix-wp-blocks": "^1.2.0",
    "@wordpress/babel-preset-default": "^7.6.0",
    "@wordpress/browserslist-config": "^5.5.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.27.10",
    "browser-sync-webpack-plugin": "^2.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-plugin-import": "^2.26.0",
    "laravel-mix": "^6.0.49",
    "npm-run-all": "^4.1.5",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "stylelint": "^14.16.0",
    "stylelint-config-standard": "^29.0.0",
    "vue-template-compiler": "^2.7.14",
    "yarn-upgrade-all": "^0.7.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@glidejs/glide": "^3.6.0",
    "bootstrap": "^5.2.3",
    "postcss": "^8.4.19"
  }
}
